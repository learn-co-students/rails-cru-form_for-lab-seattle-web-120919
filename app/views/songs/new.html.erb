<h1>My Song Name</h1>
 <p><%= form_for(@song) do |f| %>
    <%= f.label "Song:" %><br>
    <%= f.text_field :name %><br>

    <%= f.label "Artist:" %><br>
    <%= f.text_field :artist_id %><br>

    <%= f.label "Genre:" %><br>
    <%= f.text_field :genre_id %><br>
    
    <%= f.submit %>
    <% end %>
</p>